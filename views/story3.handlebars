<!DOCTYPE html>
<html>

<head>
    <title>Home Page</title>
</head>

<body>

    <h2>{{title}}</h2>



    <table>
        <thead>
            <th>First Name</th>
            <th>Last Name</th>
            <th>DOB</th>
        </thead>

        <tbody>
            {{#each results}}
            <tr>
                <td>{{this.first_name}}</td>
                <td>{{this.last_name}}</td>
                <td>{{this.dob}}</td>
            </tr>
            {{/each}}
        </tbody>


    </table>

</body>

</html>